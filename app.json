      "package": "com.nguyendaiphuc.phuckhang.test"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    
    "extra": {
      "eas": {
        "projectId": "7d4e5269-2d5c-4a86-a89f-61d585206148"
      }
